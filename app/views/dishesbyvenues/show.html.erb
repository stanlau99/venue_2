<div class="page-header">
  <h3>Dishesbyvenue #<%= @dishesbyvenue.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Venueid</dt>
      <dd>
        <% if @dishesbyvenue.venue.present? %>
          <a href="/venues/<%= @dishesbyvenue.venueid %>">
            <%= @dishesbyvenue.venue.id %>
          </a>
        <% end %>
      </dd>

      <dt>Dishid</dt>
      <dd>
        <% if @dishesbyvenue.dish.present? %>
          <a href="/dishes/<%= @dishesbyvenue.dishid %>">
            <%= @dishesbyvenue.dish.id %>
          </a>
        <% end %>
      </dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/dishesbyvenues" class="btn btn-primary">
        Back
      </a>
      <a href="/dishesbyvenues/<%= @dishesbyvenue.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_dishesbyvenue/<%= @dishesbyvenue.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
